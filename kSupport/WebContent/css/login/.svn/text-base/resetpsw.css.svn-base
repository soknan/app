.k-clear {
	clear: both;
}

#reset-footer {
	margin-top: 10px;
}

.bg_reset {
	width: 440px;
}

.k-reseted {
	padding: 100px 24px 0 100px;
}

.k-reseted-msg {
	color: #636363;
	font-family: arial, sans-serif;
	font-size: 16px;
	margin: 15px;
}

.meter {
	font-size: 1px;
	height: 3px;
}

.meter-inner {
	width: 0px;
	height: 3px;
}

.meter-red .meter-inner {
	background: red;
}

.meter-orange .meter-inner {
	background: orange;
}

.meter-green .meter-inner {
	background: green;
}

#reset_footer .submit {
	float: right;
	margin-right: 164px;
	margin-top: -10px;
}

#reset_footer .close {
	float: right;
	margin-right: 24px;
	margin-top: -10px;
}

.txtBoxForgotPsw {
	margin: 0 30px 0 0;
	width: 335px;
}

.forgot-msg {
	padding: 15px 0 0 0;
}

.k-login {
	padding: 20px;
}

.k-login-key {
	background-image: url("../img/kbureau/Unlock-64.png");
	background-position: left top;
	background-repeat: no-repeat;
	font-size: 16px;
	font-weight: bold;
	padding: 10px 10px 50px 100px;
}

.myLabel {
	color: #636363;
	font-family: arial, sans-serif;
	font-size: 12px;
	margin: 10px 0 0;
}